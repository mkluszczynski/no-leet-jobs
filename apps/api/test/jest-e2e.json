{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@lib/upload/(.*)": "<rootDir>/../libs/upload/src/$1",
    "@lib/upload": "<rootDir>/../libs/upload/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/hash/(.*)": "<rootDir>/../libs/hash/src/$1",
    "@app/hash": "<rootDir>/../libs/hash/src"
  }
}